{
    "name": "voltra/slim-vue-app",
    "description": "A good to go project setup for Slim VueJS Twig applications",
    "type": "library",
    "authors": [
        {
            "name": "Voltra",
            "email": "ludwig.guerin.98@gmx.fr"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.1.1",
        "slim/slim": "^3.11",
        "slim/twig-view": "^2.4",
        "bryanjhv/slim-session": "^3.6",
        "slim/flash": "^0.4.0",
        "kanellov/slim-twig-flash": "^0.2.0",
        "illuminate/database": "^5.5",
        "fzaninotto/faker": "^1.8",
        "illuminate/pagination": "^5.5",
        "hassankhan/config": "^0.11.2",
        "vlucas/phpdotenv": "^2.5",
        "dflydev/fig-cookies": "^1.0",
        "paragonie/random-lib": "^2.0",
        "zeuxisoo/slim-whoops": "^0.6.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "php-mock/php-mock": "^2.0",
        "php-mock/php-mock-phpunit": "^2.1.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "dev",
            "App\\Tests\\": "Tests"
        }
    }
}
