{
	"name": "voltra/slim-vue-app",
	"description": "A good to go project setup for Slim VueJS Twig applications",
	"type": "project",
    "authors": [
        {
            "name": "Voltra",
            "email": "ludwig.guerin.98@gmx.fr"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "App\\": "dev",
            "App\\Tests\\": "Tests"
        }
    },
	"require": {
		"php": ">=7.2.0",
		"slim/slim": "^4.5",
		"vlucas/phpdotenv": "^5.1",
		"slim/psr7": "^1.2",
		"illuminate/database": "^8.1",
		"nesbot/carbon": "^2.39",
		"illuminate/filesystem": "^8.1",
		"illuminate/pagination": "^8.1",
		"kitetail/zttp": "^0.6.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^9.3",
		"php-mock/php-mock": "^2.2",
		"php-mock/php-mock-phpunit": "^2.6"
	}
}
